{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "babel-plugin-styled-components",
      {
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "#components": "./src/components",
          "#pages": "./src/pages",
          "#routes": "./src/routes",
          "#services": "./src/services",
          "#static": "./src/static",
          "#store": "./src/store",
          "#style": "./src/style",
          "#utils": "./src/utils"
        }
      }
    ]
  ]
}
